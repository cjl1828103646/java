package 第七次作业;
import java.util.Date;
public class 课后练习9_3 {
	Date date = new Date();
	
	public void setDate(int n) {
		date.setTime(n);
	}
	
	public void getDate() {
		System.out.print(date.toString());
	}
	
	public static void main(String[] args) {
		Date date = new Date();
		for(long i = 10000; i <= 1e10;i=i*10) {
	            date.setTime(i);
	            System.out.println(date.toString());
	        }
	}
}
